// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+alt+/",
        "command": "workbench.action.toggleDevTools",
        "when": "false"
    },
    {
        "key": "ctrl+shift+i",
        "command": "-workbench.action.toggleDevTools",
        "when": "isDevelopment"
    },
    {
        "key": "ctrl+shift+/",
        "command": "editor.action.inspectTMScopes"
    }
]