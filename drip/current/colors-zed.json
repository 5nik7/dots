{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#14244490",
                "editor.background": "#14244490",
                "editor.foreground": "#E4F0FF",
                "text": "#E4F0FF",
                "text.muted": "#E4F0FF70",
                "text.ignored": "#E4F0FF40",
                "text.placeholder": "#E4F0FF50",
                "ignored": "#E4F0FF30",
                "element.hover": "#E4F0FF30",
                "ghost_element.hover": "#FF567330",
                "ghost_element.selected": "#FF567330",
                "ghost_element.active": "#FF567360",
                "border": "#142444",
                "editor.highlighted_line.background": "#FF567310",
                "editor.active_line.background": "#FF567310",
                "panel.background": "#14244490",
                "title_bar.background": "#14244490",
                "title_bar.inactive_background": "#14244490",
                "status_bar.background": "#14244490",
                "drop_target.background": "#14244490",
                "elevated_surface.background": "#142444",
                "toolbar.background": "#14244490",
                "tab_bar.background": "#14244490",
                "tab.inactive_background": "#14244490",
                "tab.active_background": "#FF567330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#142444",
                "scrollbar.thumb.background": "#FF5673",
                "editor.gutter.background": "#14244490",
                "terminal.background": "#14244410",
                "terminal.foreground": "#E4F0FF",
                "terminal.dim_foreground": "#E4F0FF",
                "terminal.bright_foreground": "#E4F0FF",
                "terminal.ansi.black": "#142444",
                "terminal.ansi.red": "#FF5673",
                "terminal.ansi.green": "#00FFCC",
                "terminal.ansi.yellow": "#FFAA82",
                "terminal.ansi.blue": "#3D81FF",
                "terminal.ansi.magenta": "#A798F0",
                "terminal.ansi.cyan": "#00E5FF",
                "terminal.ansi.white": "#D3EEFA",
                "terminal.ansi.bright_black": "#334775",
                "terminal.ansi.bright_red": "#FF859B",
                "terminal.ansi.bright_green": "#8FF8B5",
                "terminal.ansi.bright_yellow": "#FFC990",
                "terminal.ansi.bright_blue": "#508FF8",
                "terminal.ansi.bright_magenta": "#ABAFFF",
                "terminal.ansi.bright_cyan": "#30F1EA",
                "terminal.ansi.bright_white": "#E4F0FF",
                "modified": "#FF5673",
                "syntax": {
                    "attribute": {
                        "color": "#FF5673"
                    },
                    "boolean": {
                      "color": "#00FFCC"
                    },
                    "comment": {
                      "color": "#334775"
                    },
                    "comment.doc": {
                      "color": "#334775"
                    },
                    "constant": {
                      "color": "#8FF8B5"
                    },
                    "function": {
                      "color": "#30F1EA"
                    },
                    "keyword": {
                      "color": "#3D81FF"
                    },
                    "number": {
                      "color": "#A798F0"
                    },
                    "operator": {
                      "color": "#3D81FF"
                    },
                    "string": {
                      "color": "#FF5673"
                    },
                    "variable": {
                      "color": "#00FFCC"
                    }
                },
                "players": [
                  {
                    "cursor": "#FF5673",
                    "background": "#142444",
                    "selection": "#FF567330"
                  }
                ]
            }
        }
    ]
}
