{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#1a1b2690",
                "editor.background": "#1a1b2690",
                "editor.foreground": "#c0caf5",
                "text": "#c0caf5",
                "text.muted": "#c0caf570",
                "text.ignored": "#c0caf540",
                "text.placeholder": "#c0caf550",
                "ignored": "#c0caf530",
                "element.hover": "#c0caf530",
                "ghost_element.hover": "#f7768e30",
                "ghost_element.selected": "#f7768e30",
                "ghost_element.active": "#f7768e60",
                "border": "#1a1b26",
                "editor.highlighted_line.background": "#f7768e10",
                "editor.active_line.background": "#f7768e10",
                "panel.background": "#1a1b2690",
                "title_bar.background": "#1a1b2690",
                "title_bar.inactive_background": "#1a1b2690",
                "status_bar.background": "#1a1b2690",
                "drop_target.background": "#1a1b2690",
                "elevated_surface.background": "#1a1b26",
                "toolbar.background": "#1a1b2690",
                "tab_bar.background": "#1a1b2690",
                "tab.inactive_background": "#1a1b2690",
                "tab.active_background": "#f7768e30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#1a1b26",
                "scrollbar.thumb.background": "#f7768e",
                "editor.gutter.background": "#1a1b2690",
                "terminal.background": "#1a1b2610",
                "terminal.foreground": "#c0caf5",
                "terminal.dim_foreground": "#c0caf5",
                "terminal.bright_foreground": "#c0caf5",
                "terminal.ansi.black": "#15161e",
                "terminal.ansi.red": "#f7768e",
                "terminal.ansi.green": "#9ece6a",
                "terminal.ansi.yellow": "#e0af68",
                "terminal.ansi.blue": "#7aa2f7",
                "terminal.ansi.magenta": "#bb9af7",
                "terminal.ansi.cyan": "#7dcfff",
                "terminal.ansi.white": "#a9b1d6",
                "terminal.ansi.bright_black": "#414868",
                "terminal.ansi.bright_red": "#ff899d",
                "terminal.ansi.bright_green": "#9fe044",
                "terminal.ansi.bright_yellow": "#faba4a",
                "terminal.ansi.bright_blue": "#8db0ff",
                "terminal.ansi.bright_magenta": "#c7a9ff",
                "terminal.ansi.bright_cyan": "#a4daff",
                "terminal.ansi.bright_white": "#c0caf5",
                "modified": "#f7768e",
                "syntax": {
                    "attribute": {
                        "color": "#f7768e"
                    },
                    "boolean": {
                      "color": "#9ece6a"
                    },
                    "comment": {
                      "color": "#414868"
                    },
                    "comment.doc": {
                      "color": "#414868"
                    },
                    "constant": {
                      "color": "#9fe044"
                    },
                    "function": {
                      "color": "#a4daff"
                    },
                    "keyword": {
                      "color": "#7aa2f7"
                    },
                    "number": {
                      "color": "#bb9af7"
                    },
                    "operator": {
                      "color": "#7aa2f7"
                    },
                    "string": {
                      "color": "#f7768e"
                    },
                    "variable": {
                      "color": "#9ece6a"
                    }
                },
                "players": [
                  {
                    "cursor": "#f7768e",
                    "background": "#1a1b26",
                    "selection": "#f7768e30"
                  }
                ]
            }
        }
    ]
}
