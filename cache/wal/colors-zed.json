{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#1E1E2E90",
                "editor.background": "#1E1E2E90",
                "editor.foreground": "#CDD6F4",
                "text": "#CDD6F4",
                "text.muted": "#CDD6F470",
                "text.ignored": "#CDD6F440",
                "text.placeholder": "#CDD6F450",
                "ignored": "#CDD6F430",
                "element.hover": "#CDD6F430",
                "ghost_element.hover": "#F38BA830",
                "ghost_element.selected": "#F38BA830",
                "ghost_element.active": "#F38BA860",
                "border": "#1E1E2E",
                "editor.highlighted_line.background": "#F38BA810",
                "editor.active_line.background": "#F38BA810",
                "panel.background": "#1E1E2E90",
                "title_bar.background": "#1E1E2E90",
                "title_bar.inactive_background": "#1E1E2E90",
                "status_bar.background": "#1E1E2E90",
                "drop_target.background": "#1E1E2E90",
                "elevated_surface.background": "#1E1E2E",
                "toolbar.background": "#1E1E2E90",
                "tab_bar.background": "#1E1E2E90",
                "tab.inactive_background": "#1E1E2E90",
                "tab.active_background": "#F38BA830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#1E1E2E",
                "scrollbar.thumb.background": "#F38BA8",
                "editor.gutter.background": "#1E1E2E90",
                "terminal.background": "#1E1E2E10",
                "terminal.foreground": "#CDD6F4",
                "terminal.dim_foreground": "#CDD6F4",
                "terminal.bright_foreground": "#CDD6F4",
                "terminal.ansi.black": "#45475A",
                "terminal.ansi.red": "#F38BA8",
                "terminal.ansi.green": "#A6E3A1",
                "terminal.ansi.yellow": "#F9E2AF",
                "terminal.ansi.blue": "#89B4FA",
                "terminal.ansi.magenta": "#F5C2E7",
                "terminal.ansi.cyan": "#94E2D5",
                "terminal.ansi.white": "#BAC2DE",
                "terminal.ansi.bright_black": "#585B70",
                "terminal.ansi.bright_red": "#F38BA8",
                "terminal.ansi.bright_green": "#A6E3A1",
                "terminal.ansi.bright_yellow": "#F9E2AF",
                "terminal.ansi.bright_blue": "#89B4FA",
                "terminal.ansi.bright_magenta": "#F5C2E7",
                "terminal.ansi.bright_cyan": "#94E2D5",
                "terminal.ansi.bright_white": "#A6ADC8",
                "modified": "#F38BA8",
                "syntax": {
                    "attribute": {
                        "color": "#F38BA8"
                    },
                    "boolean": {
                      "color": "#A6E3A1"
                    },
                    "comment": {
                      "color": "#585B70"
                    },
                    "comment.doc": {
                      "color": "#585B70"
                    },
                    "constant": {
                      "color": "#A6E3A1"
                    },
                    "function": {
                      "color": "#94E2D5"
                    },
                    "keyword": {
                      "color": "#89B4FA"
                    },
                    "number": {
                      "color": "#F5C2E7"
                    },
                    "operator": {
                      "color": "#89B4FA"
                    },
                    "string": {
                      "color": "#F38BA8"
                    },
                    "variable": {
                      "color": "#A6E3A1"
                    }
                },
                "players": [
                  {
                    "cursor": "#F38BA8",
                    "background": "#1E1E2E",
                    "selection": "#F38BA830"
                  }
                ]
            }
        }
    ]
}
