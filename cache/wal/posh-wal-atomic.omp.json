{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "background": "#89B4FA",
            "foreground": "#BAC2DE",
            "leading_diamond": "\u256d\u2500\ue0b6",
            "style": "diamond",
            "template": "\uf120 {{ .Name }} ",
            "type": "shell"
          },
          {
            "background": "#F38BA8",
            "foreground": "#F9E2AF",
            "style": "diamond",
            "template": "<parentBackground>\ue0b0</> \uf292 ",
            "type": "root"
          },
          {
            "background": "#F38BA8",
            "foreground": "#45475A",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "folder_icon": " \uf07b ",
              "home_icon": "\ue617",
              "style": "folder"
            },
            "style": "powerline",
            "template": " \uf07b\uea9c {{ .Path }} ",
            "type": "path"
          },
          {
            "background": "#F9E2AF",
            "background_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#F9E2AF{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#A6E3A1{{ end }}",
              "{{ if gt .Ahead 0 }}#F5C2E7{{ end }}",
              "{{ if gt .Behind 0 }}#F5C2E7{{ end }}"
            ],
            "foreground": "#45475A",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "branch_icon": "\ue725 ",
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "powerline",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#F38BA8> \uf046 {{ .Staging.String }}</>{{ end }} ",
            "type": "git"
          },
          {
            "background": "#585B70",
            "foreground": "#BAC2DE",
            "properties": {
              "style": "roundrock",
              "threshold": 0
            },
            "style": "diamond",
            "template": " \ueba2 {{ .FormattedMs }}\u2800",
            "trailing_diamond": "\ue0b4",
            "type": "executiontime"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "background": "#45475A",
            "foreground": "#A6E3A1",
            "leading_diamond": "\ue0b6",
            "properties": {
              "fetch_package_manager": true,
              "npm_icon": " <#F38BA8>\ue5fa</> ",
              "yarn_icon": " <#89B4FA>\ue6a7</>"
            },
            "style": "diamond",
            "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "node"
          },
          {
            "background": "#89B4FA",
            "foreground": "#F9E2AF",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "python"
          },
          {
            "background": "#89B4FA",
            "foreground": "#BAC2DE",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "java"
          },
          {
            "background": "#45475A",
            "foreground": "#89B4FA",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "dotnet"
          },
          {
            "background": "#BAC2DE",
            "foreground": "#94E2D5",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "go"
          },
          {
            "background": "#A6ADC8",
            "foreground": "#F9E2AF",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "rust"
          },
          {
            "background": "#A6ADC8",
            "foreground": "#89B4FA",
            "leading_diamond": " \ue0b6",
            "style": "diamond",
            "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "dart"
          },
          {
            "background": "#BAC2DE",
            "foreground": "#F38BA8",
            "leading_diamond": " \ue0b6",
            "style": "diamond",
            "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "angular"
          },
          {
            "background": "#585B70",
            "foreground": "#BAC2DE",
            "leading_diamond": " \ue0b6",
            "style": "diamond",
            "template": "{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "nx"
          },
          {
            "background": "#F5C2E7",
            "foreground": "#A6E3A1",
            "leading_diamond": " \ue0b6",
            "style": "diamond",
            "template": "<#F38BA8>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "julia"
          },
          {
            "background": "#BAC2DE",
            "foreground": "#F38BA8",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "ruby"
          },
          {
            "background": "#BAC2DE",
            "foreground": "#94E2D5",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\uf104<#F38BA8>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
            "trailing_diamond": "\ue0b4 ",
            "type": "azfunc"
          },
          {
            "background": "#585B70",
            "foreground": "#F38BA8",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
            "trailing_diamond": "\ue0b4 ",
            "type": "aws"
          },
          {
            "background": "#89B4FA}",
            "foreground": "#BAC2DE",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template": "\uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
            "trailing_diamond": "\ue0b4",
            "type": "kubectl"
          },
          {
            "background": "#585B70",
            "foreground": "#45475A",
            "leading_diamond": "\ue0b6",
            "trailing_diamond": "<transparent,background>\ue0b2</>",
            "properties": {
              "linux": "\ue712",
              "macos": "\ue711",
              "windows": "\ue70f"
            },
            "style": "diamond",
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
            "type": "os"
          },
          {
            "background": "#F38BA8",
            "background_templates": [
              "{{if eq \"Charging\" .State.String}}#A6E3A1{{end}}",
              "{{if eq \"Discharging\" .State.String}}#F9E2AF}{{end}}",
              "{{if eq \"Full\" .State.String}}#A6E3A1{{end}}"
            ],
            "foreground": "#585B70",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "properties": {
              "charged_icon": "\uf240 ",
              "charging_icon": "\uf1e6 ",
              "discharging_icon": "\ue234 "
            },
            "style": "powerline",
            "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
            "type": "battery"
          },
          {
            "background": "#94E2D5",
            "foreground": "#BAC2DE",
            "invert_powerline": true,
            "leading_diamond": "\ue0b2",
            "properties": {
              "time_format": "_2,15:04"
            },
            "style": "diamond",
            "template": " \uf073 {{ .CurrentDate | date .Format }} ",
            "trailing_diamond": "\ue0b4",
            "type": "time"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#94E2D5",
            "style": "plain",
            "template": "\u2570\u2500",
            "type": "text"
          },
          {
            "foreground": "#A6ADC8",
            "foreground_templates": ["{{ if gt .Code 0 }}#F38BA8{{ end }}"],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "\ue285\ueab6 ",
            "type": "status"
          }
        ],
        "type": "prompt"
      }
    ],
    "version": 2
  }